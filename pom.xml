    </license>
  </licenses>
  <developers>
    <developer>
      <id>developer</id>
      <name>Developer Name</name>
      <email>developer@example.com</email>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <groupId>androidx.legacy</groupId>
